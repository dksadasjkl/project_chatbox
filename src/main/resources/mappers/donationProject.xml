<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.give.repository.DonationProjectMapper">

    <insert id="insertDonationProject" parameterType="com.project.give.entity.DonationProject" useGeneratedKeys="true" keyProperty="donationProjectId"><![CDATA[
        insert into
            donation_project_tb
        values
            (   0,
                #{donationProjectTitle},
                #{donationProjectOrganization},
                #{donationProjectImageUrl},
                #{donationCategoryId},
                #{donationProjectCurrentAmount},
                #{donationProjectTargetAmount},
                now(),
                now(),
                now(),
                now()
            )
    ]]></insert>


</mapper>